<% form_for(@intake) do |f| %>
	<%= f.error_messages %>
	<div class="tform">
		<H2><%=t('intake.edit')%></H2>
		<%=render :partial => 'form', :locals => {:f => f} %>
	</div>
  	<p>
    	<%= f.submit (t 'update') %>
  	</p>
<% end %>

<%= link_to (t 'show'), @intake %> |
<%= link_to (t 'back'), intakes_path %>