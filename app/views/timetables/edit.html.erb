<% form_for(@timetable) do |f| %>
  <%= f.error_messages %>
	<div class="tform">
		<H2><%=(t 'timetable.edit') %></H2>
		<%=render :partial => 'form', :locals => {:f => f} %>
	</div>
  <p>
    <%= f.submit (t 'update') %>
  </p>
<% end %>

<%= link_to (t 'show'), @timetable %> |
<%= link_to (t 'back'), timetables_path %>