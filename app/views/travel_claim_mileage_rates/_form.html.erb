<table width=80%>
	<tr>
		<th width=10% rowspan="2">KM <%= t 'from' %></th>
		<th width=10% rowspan="2">KM <%= t 'to' %></th>
		<th colspan="5"><center><%=t('mileagerates.transport_group')%></center></th>
	</tr>
	<tr>
		
		<th width=10%>A</th>
		<th width=10%>B</th>
		<th width=10%>C</th>
		<th width=10%>D</th>
		<th width=10%>E</th>
	</tr>
	<tr>
		<td><%= f.text_field :km_low,  :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :km_high, :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :a_group, :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :b_group, :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :c_group, :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :d_group, :size => 5, :class => 'numero' %></td>
		<td><%= f.text_field :e_group, :size => 5, :class => 'numero' %></td>
	</tr>

</table>